{
    "name": "projectify-frontend",
    "version": "2024.8.20",
    "scripts": {
        "build": "vite build",
        "check": "run-p --max-parallel 4 check:*",
        "check:eslint": "eslint --cache --cache-strategy content --ext .ts,.js,.svelte src",
        "check:knip": "knip",
        "check:prettier": "npm run prettier -- --cache --cache-strategy content --check .",
        "check:schema": "bin/update-schema && git diff --exit-code src/lib/types/schema.d.ts",
        "check:svelte-check": "svelte-check --tsconfig tsconfig.json --fail-on-warnings",
        "dev": "vite dev --host --port 3000",
        "eslint": "eslint --ext .ts,.js,.svelte src",
        "fix": "run-p --max-parallel 4 fix:*",
        "fix:eslint": "npm run eslint -- --fix --cache --cache-strategy content",
        "fix:prettier": "npm run prettier -- --cache --cache-strategy content --write .",
        "fix:schema": "bin/update-schema",
        "format": "npm run fix:prettier",
        "nocache:check": "run-p --max-parallel 4 nocache:check:* check:svelte-check check:schema check:knip",
        "nocache:check:eslint": "eslint --ext .ts,.js,.svelte src",
        "nocache:check:prettier": "npm run prettier -- --check .",
        "nocache:fix": "run-p --max-parallel 4 nocache:fix:* fix:schema",
        "nocache:fix:eslint": "npm run eslint --fix",
        "nocache:fix:prettier": "npm run prettier -- --write .",
        "package": "vite package",
        "prettier": "prettier --ignore-path .prettierignore --ignore-path ../.gitignore --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --log-level warn",
        "preview": "vite preview --port 3000 --outDir build",
        "show:bundle:client": "open .svelte-kit/output/client/bundle.html",
        "show:bundle:server": "open .svelte-kit/output/server/bundle.html",
        "svelteserver": "svelteserver",
        "sync-types": "svelte-kit sync",
        "test": "bin/test",
        "typescript-language-server": "typescript-language-server"
    },
    "devDependencies": {
        "@sveltejs/vite-plugin-svelte": "~3.1.1",
        "@types/pikaday": "~1.7.9",
        "@typescript-eslint/eslint-plugin": "^7",
        "@typescript-eslint/parser": "^7",
        "eslint": "^8.39",
        "eslint-config-prettier": "^9.0.0",
        "eslint-import-resolver-typescript": "~3.5.5",
        "eslint-plugin-import": "~2.27.5",
        "eslint-plugin-svelte": "~2.32.4",
        "eslint-plugin-tsdoc": "~0.2.17",
        "esm-loader-import-alias": "~1.0.9",
        "esm-loader-import-relative-extension": "~1.0.11",
        "esm-loader-typescript": "~1.0.6",
        "knip": "~5.33.3",
        "node-esm-loader": "~0.3.0",
        "npm-run-all2": "~6.2.0",
        "openapi-typescript": "~6.7.6",
        "prettier": "^3.0.3",
        "prettier-plugin-svelte": "^3.0.3",
        "prettier-plugin-tailwindcss": "^0.5.14",
        "svelte-check": "~3.4.6",
        "svelte-eslint-parser": "~0.33.1",
        "svelte-language-server": "^0.16",
        "typescript-language-server": "~4.3.3",
        "typescript-svelte-plugin": "~0.3.38"
    },
    "type": "module",
    "dependencies": {
        "@anephenix/sarus": "~0.5",
        "@poppanator/sveltekit-svg": "~4.2.1",
        "@steeze-ui/heroicons": "~1.1.1",
        "@steeze-ui/svelte-icon": "~1.5.0",
        "@sveltejs/adapter-node": "~5.2.0",
        "@sveltejs/adapter-static": "~3.0.1",
        "@sveltejs/enhanced-img": "~0.2.1",
        "@sveltejs/kit": "~2.5.10",
        "@tailwindcss/typography": "~0.5.13",
        "autoprefixer": "~10.4.19",
        "date-fns": "~2.30.0",
        "exponential-backoff": "^3.1.1",
        "favicons": "~7.2.0",
        "fuse.js": "~7.0",
        "generate-license-file": "^3.4",
        "marked": "~12.0.2",
        "marked-gfm-heading-id": "~3.1.3",
        "openapi-fetch": "~0.9.7",
        "pikaday": "~1.8.2",
        "rollup-plugin-visualizer": "~5.12",
        "sass": "^1.77",
        "superlock": "^1.2.1",
        "svelte": "~4.2.19",
        "svelte-i18n": "~4.0.0",
        "svelte-local-storage-store": "~0.6.4",
        "svelte-sitemap": "~2.6.0",
        "tailwindcss": "^3.4",
        "typescript": "~5.2.2",
        "vite": "~5.2.12"
    }
}
